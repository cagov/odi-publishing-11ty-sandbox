{
    "version": "2.0.0",
    "name": "odi-publishing-sandbox.ca.gov - Office of Data and Innovation",
    "gitrepo": "@cagov/odi-publishing-11ty-sandbox",
    "contact": {
        "maintainer": "Office of Data and Innovation"
    },
    "page_metadata": {
        "en": {
            "site_url": "http://odi-publishing-11ty-sandbox.ca.gov.s3-website-us-west-1.amazonaws.com",
            "site_name": "Office of Data and Innovation",
            "site_default_description": "ODI Publishing test instance",
            "page_default_open_graph_image_url": "./assets/default-social-media-image.png",
            "page_default_open_graph_image_alt": "Office of Data and Innovation",
            "page_default_open_graph_image_width": 1200,
            "page_default_open_graph_image_height": 630,
            "gov_name": "CA.GOV",
            "gov_url": "https:\/\/ca.gov",
            "favicon": "/assets/cagov_favicon.ico",
            "page_icon": "/assets/img/cagov_favicon_16x16.png",
            "keywords": "California, government"
        }
    },
    "locales": {
        "en": "English"
    },
    "extensions": {
        "page-feedback": {
            "endpoint": "https://fa-go-feedback-001.azurewebsites.net/sendfeedback"
        }
    },
    "build": {
        "eleventy_input": "pages",
        "eleventy_output": "docs",
        "eleventy_layouts": "_includes/layouts",
        "eleventy_content": "pages/_content",
        "eleventy_pages": "pages/_content/pages",
        "eleventy_posts": "pages/_content/posts",
        "eleventy_menu": "pages/_content/menu",
        "eleventy_redirects": "pages/_content/redirects",
        "docs_media": "wp-content/uploads/sites/2",
        "upload_folder": "wp-content/uploads/",
        "replace_urls": [
            "https://editor-dev-cannabis-ca-gov.pantheonsite.io/"
        ],
        "media_replace_urls": [
            "https://editor-dev-cannabis-ca-gov.pantheonsite.io/wp-content/uploads/"
        ],
        "editor_url": "https://editor-dev-cannabis-ca-gov.pantheonsite.io",
        "canonical_site_url": "http://odi-publishing-11ty-sandbox.ca.gov.s3-website-us-west-1.amazonaws.com",
        "host_name": "odi-publishing-11ty-sandbox.ca.gov.s3-website-us-west-1.amazonaws.com"
    }
}