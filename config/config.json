{
    "version": "2.0.0",
    "name": "example.ca.gov - Office of Data and Innovation",
    "gitrepo": "@cagov/example.ca.gov",
    "contact": {
        "maintainer": "Office of Data and Innovation"
    },
    "page_metadata": {
        "en": {
            "site_url": "https://example.ca.gov",
