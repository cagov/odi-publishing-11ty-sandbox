{
  "name": "odi-publishing-11ty-sandbox",
  "version": "2.0.0",
  "description": "Static site generator for test site",
  "main": "index.html",
  "scripts": {
    "build": "eleventy",
    "dev": "npm run site:watch:dev",
    "site:build:dev": "cross-env NODE_ENV=development SITE_ENV=localhost DOMAIN=localhost eleventy",
    "site:build:production": "cross-env NODE_ENV=production SITE_ENV=production DOMAIN=odi-publishing-11ty-sandbox.ca.gov eleventy",
    "site:watch:dev": "cross-env NODE_ENV=development SITE_ENV=localhost eleventy --watch --serve --incremental",
    "site:watch": "eleventy --serve",
    "start": "npm run site:watch",
    "deploy:redirects": "node src/deploy/redirects/assign.js",
    "test:setup": "npx playwright install-deps chromium && npx playwright install",
    "test:serve": "npx http-server --port 8080 docs/",
    "test": "npx playwright test test/e2e.spec.mjs --project=chromium --config test/playwright.e2e.config.js",
    "pdfcheck:dev": "npx playwright test test/pdfcheck.spec.mjs --config=test/playwright-dev.config.js",
    "linkcheck:dev": "npx playwright test test/linkcheck.spec.mjs --config=test/playwright-dev.config.js",
    "linkcheck:gh": "npx playwright test test/linkcheck.spec.mjs --config=test/playwright.config.js",
    "format:prettier": "prettier \"**/*.js\" --write --ignore-path .gitignore",
    "code:format": "npm run format:prettier --ignore-path",
    "content:clean": "rm -rf docs && rm -rf pages/_content/menu && rm -rf pages/_content/pages && rm -rf pages/_content/posts && rm -rf pages/_content/redirects",
    "content:update": "npm install odi-publishing-static-test-content --production --no-package-lock",
    "content:update:raw": "npm install https://github.com/cagov/odi-publishing-static-test-content/tarball/main-reduced --save",
    "install:codetools": "npm install group-dependencies && deps install test"
  },
  "keywords": [
    "@cagov",
    "Office of Data and Innovation"
  ],
  "repository": "https://github.com/odi-publishing-11ty-sandbox",
  "author": "Office of Data and Innovation",
  "license": "MIT",
  "dependencies": {
    "@11ty/eleventy": "^2.0.0-canary.15",
    "@cagov/11ty-build-system": "^1.0.0-beta.2",
    "@cagov/ds-accordion": "^2.0.3",
    "@cagov/ds-back-to-top": "^2.0.2",
    "@cagov/ds-base-css": "^2.0.0-beta.1",
    "@cagov/ds-branding": "^1.0.8",
    "@cagov/ds-feature-card": "^2.0.0",
    "@cagov/ds-link-grid": "^3.0.0",
    "@cagov/ds-link-icon": "^2.0.0",
    "@cagov/ds-page-alert": "^2.0.2",
    "@cagov/ds-page-feedback": "^1.1.3",
    "@cagov/ds-page-navigation": "^2.2.0",
    "@cagov/ds-pagination": "^2.0.2",
    "@cagov/ds-regulatory-outline": "^1.0.1",
    "@cagov/ds-site-footer": "^2.0.0",
    "@cagov/ds-site-header": "^2.0.0",
    "@cagov/ds-site-navigation": "^3.0.0",
    "@cagov/ds-skip-to-content": "^2.0.0",
    "@cagov/ds-statewide-footer": "^2.0.0",
    "@cagov/ds-statewide-header": "^2.0.0",
    "@cagov/ds-step-list": "^3.0.0",
    "@cagov/ds-table": "^1.0.0",
    "aws-sdk": "^2.1200.0",
    "cheerio": "^1.0.0-rc.12",
    "cross-env": "^7.0.3",
    "odi-publishing-static-test-content": "https://github.com/cagov/odi-publishing-static-test-content/tarball/main-reduced"
  },
  "devDependencies": {
    "@open-wc/eslint-config": "^4.3.0",
    "@open-wc/testing": "^3.1.6",
    "@playwright/test": "^1.25.2",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "clean-css": "^5.3.1",
    "eslint-config-prettier": "^8.3.0",
    "html-minifier": "^4.0.0",
    "rollup-plugin-terser": "^7.0.2"
  }
}
